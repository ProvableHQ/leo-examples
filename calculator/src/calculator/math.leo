// Implements addition.
inline add(op0: u32, op1: u32) -> u32 {
  return op0 + op1;
}

// Implements subtraction.
inline sub(op0: u32, op1: u32) -> u32 {
  return op0 - op1;
}

// Implements multiplication.
inline mul(op0: u32, op1: u32) -> u32 {
  return op0 * op1;
}

// Implements division.
inline div(op0: u32, op1: u32) -> u32 {
  return op0 / op1;
}





